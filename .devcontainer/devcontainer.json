{
  "name": "neural-api: Free Pascal",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/neural-api",
  "postCreateCommand": "fpc -v || true",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "alefragnani.pascal",
        "davidanson.vscode-markdownlint",
        "ms-vscode.vscode-container-tools"
      ]
    }
  }
}
